{
    "$schema": "./schema.json",
    "amend": {
        "class": {
            "Actor": {
                "function": {
                    "allocate": {
                        "param": {
                            "flags": {
                                "optional": true
                            }
                        }
                    },
                    "constructor": {
                        "newParam": [
                            {
                                "name": "options",
                                "optional": true,
                                "type": "Partial<ActorOptions>"
                            }
                        ]
                    },
                    "get_child_at_index": {
                        "return_type": {
                            "type": "T"
                        },
                        "generic": "<T = Actor>"
                    },
                    "get_children": {
                        "return_type": {
                            "type": "T"
                        },
                        "generic": "<T = Actor[]>"
                    }
                },
                "prop": {
                    "x_align": {
                        "type_extension": [
                            "St.Align"
                        ]
                    },
                    "y_align": {
                        "type_extension": [
                            "St.Align"
                        ]
                    }
                }
            },
            "GridLayout": {
                "function": {
                    "constructor": {
                        "newParam": [
                            {
                                "name": "options",
                                "optional": true,
                                "type": "any"
                            }
                        ]
                    }
                }
            },
            "Point": {
                "function": {
                    "constructor": {
                        "newParam": [
                            {
                                "name": "options",
                                "type": "Partial<PointOptions>",
                                "optional": true
                            }
                        ]
                    }
                }
            },
            "Text": {
                "function": {
                    "new": {
                        "return_type": {
                            "type": "Text"
                        }
                    },
                    "new_full": {
                        "return_type": {
                            "type": "Text"
                        }
                    },
                    "new_with_buffer": {
                        "return_type": {
                            "type": "Text"
                        }
                    },
                    "new_with_text": {
                        "return_type": {
                            "type": "Text"
                        }
                    }
                }
            }
        }
    }
}
