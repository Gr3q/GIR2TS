{
    "$schema": "./schema.json",
    "amend": {
        "class": {
            "File": {
                "function": {
                    "replace_contents": {
                        "param": {
                            "contents": {
                                "type": "string"
                            },
                            "length": {
                                "skip": true
                            }
                        }
                    },
                    "load_contents": {
                        "return_type": {
                            "type": "[success: boolean, contents: Uint8Array]"
                        }
                    },
                    "delete_async": {
                        "param": {
                            "io_priority": {
                                "type_extension": ["null"]
                            }
                        }
                    },
                    "query_info_async": {
                        "param": {
                            "io_priority": {
                                "type_extension": ["null"]
                            }
                        }
                    },
                    "replace_readwrite_async": {
                        "param": {
                            "flags": {
                                "type_extension": ["null"]
                            },
                            "io_priority": {
                                "type_extension": ["null"]
                            }
                        }
                    }
                }
            },
            "IOStream": {
                "function": {
                    "close_async": {
                        "param": {
                            "io_priority": {
                                "type_extension": ["null"]
                            }
                        }
                    }
                }
            },
            "InputStream": {
                "function": {
                    "close_async": {
                        "param": {
                            "io_priority": {
                                "type_extension": ["null"]
                            }
                        }
                    }
                }
            },
            "OutputStream": {
                "function": {
                    "close_async": {
                        "param": {
                            "io_priority": {
                                "type_extension": ["null"]
                            }
                        }
                    },
                    "write_bytes_async": {
                        "param": {
                            "io_priority": {
                                "type_extension": ["null"]
                            }
                        }
                    },
                    "write": {
                        "param": {
                            "count": {
                                "skip": true
                            }
                        }
                    },
                    "write_all": {
                        "param": {
                            "count": {
                                "skip": true
                            }
                        }
                    },
                    "write_all_async": {
                        "param": {
                            "count": {
                                "skip": true
                            }
                        }
                    },
                    "write_all_finish": {
                        "param": {
                            "count": {
                                "skip": true
                            }
                        }
                    },
                    "write_async": {
                        "param": {
                            "count": {
                                "skip": true
                            }
                        }
                    }
                }
            }
        }
    }
}
