{
  "name": "gir2ts",
  "main": "./dist/generate.js",
  "scripts": {
    "start": "npm run clean && npm run build && node ./dist/generate.js --girdir=../gir/ --modifiersdir=../modifiers/ --overridesdir=../exceptions/ --outdir=../typings/",
    "build": "tsc -p ./src/tsconfig.json",
    "validate": "tsc -p tsconfig.json",
    "clean": "rm -rf ./dist"
  },
  "dependencies": {
    "minimist": "^1.2.5",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.2",
    "@types/node": "^16.11.6",
    "@types/xml2js": "^0.4.9",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
